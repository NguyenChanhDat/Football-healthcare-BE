type Query {
  getUserById(userId: String!): User
  getExerciseById(exerciseId: String!): Exercise
  getAllExercises: [Exercise]
}

type Exercise {
  id: String!
  name: String
  imageUrl: String
  targetMuscle1: String
  targetMuscle2: String
  targetMuscle3: String
}

type User {
  id: String!
  username: String!
  password: String!
  imageUrl: String
  status: Boolean!
}

type Set {
  id: String!
  exerciseName: String!
  weight: Float!
  reps: Int!
  restTime: Int!
}
